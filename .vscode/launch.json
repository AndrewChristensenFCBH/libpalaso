	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch unit tests",
			"type": "mono",
			"request": "launch",
			"program": "${workspaceRoot}/packages/NUnit.Runners.Net4.2.6.4/tools/nunit.exe",
			"args": [
				"*Tests.dll"
			],
			"cwd": "${workspaceRoot}/output/DebugMono",
			"console": "integratedTerminal"
		},
		{
			"name": "Attach",
			"type": "mono",
			"request": "attach",
			"address": "localhost",
			"port": 55555
		}
	]
}